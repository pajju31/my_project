  , "http://www.hiretale.com/files/resize_logo/13266f"
  ];


  function getImageHtmlCode() {
    var dataIndex = Math.floor(Math.random() * imageUrls.length);
    var img = '<a href=\"' + imageUrls[dataIndex] + '"><img src="'; 
    img += imageUrls[dataIndex];
    img +='"></a>';
    return img;
  }
</script>

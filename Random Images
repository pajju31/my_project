<script type="text/javascript">
  var imageUrls = [
  "http://images.clipartpanda.com/smiley-face-clip-art-thumbs-up-215700.png"
  , "http://i1.cpcache.com/product_zoom/510414468/cowboy_smiley_face_aluminum_license_plate.jpg?height=250&width=250&padToSquare=true"
  , "http://www.clipartbest.com/cliparts/bRi/dEg/bRidEgBc9.jpeg"
  , "http://i1.cpcache.com/product_zoom/703046362/smiley_face_large_glasses_225_button.jpg?height=250&width=250&padToSquare=true"
  , "http://happyendings.blog.se/files/2013/04/1_wilson-smile-frown-damp.jpg"
  , "http://www.rockswallpaperhd.com/wp-content/uploads/2013/11/Sad-Face-and-Smiley-Faces-HD-Wallpapers-300x250.jpg"
  , "https://upload.wikimedia.org/wikipedia/commons/9/9e/Lilyu_-_smile.svg"
